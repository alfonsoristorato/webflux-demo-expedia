{
  "tokenProvider" : {
    "keyProvider" : {
      "algorithm" : "ES384"
    }
  },
  "interactiveLogin": false,
  "httpServer": "NettyWrapper",
  "tokenCallbacks": [
    {
      "issuerId": "mocked",
      "tokenExpiry": 3600,
      "requestMappings": [
        {
          "requestParam": "scope",
          "match": "valid",
          "claims": {
            "sub": "valid"
          }
        },
        {
          "requestParam": "scope",
          "match": "invalid",
          "claims": {
            "sub": "invalid"
          }
        }
      ]
    }
  ]
}
